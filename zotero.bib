
@article{bennie_precise_2014,
  title = {Precise Wavefunction Engineering with Magnetic Resonance},
  abstract = {Controlling quantum fluids at their fundamental length scale will yield superlative quantum simulators, precision sensors, and spintronic devices. This scale is typically below the optical diffraction limit, precluding precise wavefunction engineering using optical potentials alone. We present a protocol to rapidly control the phase and density of a quantum fluid down to the healing length scale using strong time-dependent coupling between internal states of the fluid in a magnetic field gradient. We demonstrate this protocol by simulating the creation of a single stationary soliton and double soliton states in a Bose-Einstein condensate with control over the individual soliton positions and trajectories, using experimentally feasible parameters. Such states are yet to be realized experimentally, and are a path towards engineering soliton gases and exotic topological excitations.},
  timestamp = {2016-12-19T18:32:19Z},
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1412.6854},
  primaryClass = {cond-mat, physics:quant-ph},
  urldate = {2016-12-19},
  journal = {arXiv:1412.6854 [cond-mat, physics:quant-ph]},
  author = {Bennie, L. M. and Wigley, P. B. and Szigeti, S. S. and Jasperse, M. and Hope, J. J. and Turner, L. D. and Anderson, R. P.},
  month = dec,
  year = {2014},
  keywords = {Condensed Matter - Quantum Gases,Quantum Physics},
  file = {arXiv\:1412.6854 PDF:/home/cjb7/.zotero/zotero/dm8cibo4.default/zotero/storage/ZF9KX2IS/Bennie et al. - 2014 - Precise wavefunction engineering with magnetic res.pdf:application/pdf;arXiv.org Snapshot:/home/cjb7/.zotero/zotero/dm8cibo4.default/zotero/storage/MSJURBAR/1412.html:text/html},
  groups = {Numerics}
