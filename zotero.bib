  file = {Full Text PDF:/home/cjb7/.zotero/zotero/dm8cibo4.default/zotero/storage/VD3I2A2K/Urban et al. - 2009 - Observation of Rydberg blockade between two atoms.pdf:application/pdf;Snapshot:/home/cjb7/.zotero/zotero/dm8cibo4.default/zotero/storage/Q48E9ZBH/nphys1178.html:text/html},
  groups = {Numerics}
